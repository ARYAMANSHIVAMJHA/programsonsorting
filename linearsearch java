class searching
{  
 int search(int ar[], int x) 
{ 
    int n = ar.length; 
    for(int i = 0; i < n; i++) 
    { 
        if(ar[i] == x) 
        return i; 
    } 
    return -1; 
} 
public static void main(String args[]) 
{ 
    int ar[] = { 1, 3, 5, 15, 45 };  
    int x = 10; 
     int result = search(ar, x); 
    if(result == -1) 
     System.out.print("Element is not present in array"); 
    else
    System.out.print("Element is present at index " + result); 
} 
} 
